{
  "name": "test-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "auth": "cd auth && npm start",
    "rest-api": "cd rest-api && npm start",
    "start": "concurrently --kill-others-on-fail \"npm run auth\" \"npm run rest-api\"",
    "stop": "taskkill /F /IM node.exe",
    "install:main": "npm i",
    "install:auth": "cd auth && npm i",
    "install:rest-api": "cd rest-api && npm i",
    "install:all": "npm run install:auth && npm run install:rest-api && npm run install:main"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
